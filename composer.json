{
    "name": "renatomarinho/laravel-wave-test",
    "description": "Automatically generates tests for Laravel applications",
    "type": "library",
    "license": "Apache-2.0",
    "version": "1.0.1",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenatoMarinho\\LaravelWaveTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RenatoMarinho\\LaravelWaveTest\\LaravelWaveTestServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Renato Marinho"
        }
    ],
    "minimum-stability": "stable"
}
